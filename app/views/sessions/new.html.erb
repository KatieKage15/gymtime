<h2> Sign In </h2>

  <%=form_tag('/signin', method: 'post') do%>

  <%= label_tag :username %><br>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= password_field_tag :password %><br>
  <%= submit_tag "Sign In"%>
  <% end %>

  <p><%= link_to "Sign in via GitHub", "/auth/github" %> </p>
