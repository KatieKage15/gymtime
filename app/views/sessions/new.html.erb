<h2> Sign In </h2>

  <%= form_for @client, url: signin_path, method: :post do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.submit "Signin"%>
  <% end %>

  <p><%= link_to "Sign in via GitHub", "/auth/github" %> </p>
