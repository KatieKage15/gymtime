<h1> Sign In </h1>
    <br>
    <%= form_tag("/signin", method: "post") do %>
    <h2>Your username: </h2>
    <%= collection_select(:client, :username, Client.all, :username, :username) %><br />
    <h2>Your password: </h2>
    <%= password_field_tag :password%><br><br>
    <%= submit_tag "Sign In"%>

  <% end %>
