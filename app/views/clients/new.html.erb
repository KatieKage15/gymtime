<h1>Create a New Client</h1>

<%= form_for @client do |f| %>
<%= f.label :email %>
<%= f.text_field :email %><br />
  <%= f.label :username %>
  <%= f.text_field :username %><br />
  <%= f.label :password %>
  <%= f.password_field :password_digest %><br />
  <br />
  <%= f.submit %>
<% end %>
</br>


<p>Already have an Account? <%= link_to 'Login', signin_path %></p>
